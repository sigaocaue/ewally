{
  "name": "ewally",
  "version": "1.0.0",
  "description": "Resolução do teste para a empresa Ewally.",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/main.js",
    "dev": "node -r ./node_modules/ts-node/register -r ./node_modules/tsconfig-paths/register ./src/main.ts",
    "mock": "node -r ./node_modules/ts-node/register -r ./node_modules/tsconfig-paths/register ./src/infrastructure/mock",
    "build": "npm run build:types && npm run build:js && cp \"package.json\" \"dist/\"",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored --source-maps inline",
    "compile": "./node_modules/.bin/tsc --skipLibCheck --noEmit",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run compile",
      "post-merge": "npm install && npm audit fix && npm run compile"
    }
  },
  "keywords": [
    "ewally",
    "barCode",
    "código de barras"
  ],
  "author": "Caue Prado <caue.prado0@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@hapi/boom": "^9.1.1",
    "body-parser": "^1.19.0",
    "boleto-brasileiro-validator": "^1.0.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "faker": "^5.4.0",
    "glob": "^7.1.5",
    "lowdb": "^1.0.0",
    "moment": "^2.27.0",
    "morgan": "^1.9.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.4",
    "@babel/plugin-transform-typescript": "^7.11.0",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.4",
    "@types/body-parser": "^1.17.1",
    "@types/code": "^4.0.5",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/faker": "^5.1.7",
    "@types/glob": "^7.1.1",
    "@types/jest": "^26.0.20",
    "@types/lowdb": "^1.0.9",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.12.5",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "3.1.0",
    "jest": "^26.6.3",
    "prettier": "^2.0.5",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.3",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-jest": "^0.0.1",
    "typescript": "^3.9.5"
  }
}
